/ {
    buttons {
        compatible = "gpio-keys";
        button0: button_0 {
            gpios = <&xiao_d 10 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
            label = "Push button switch 0";
        };
    };

    aliases {
        sw0 = &button0;
        button-sw = &button0;
        button-led = &led2;
    };
};
